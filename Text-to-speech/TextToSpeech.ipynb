   "source": [
    "def tts(text, i):\n",
    "    tts = gTTS(text, 'de')  \n",
    "    tts.save('out'+str(i)+\".mp3\")\n",
    "    playsound('out'+str(i)+\".mp3\")\n",
    "\n",
    "tts('es gab eine abstimmung zu diesem punkt',2)"
